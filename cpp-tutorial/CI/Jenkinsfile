node {
     stage ('Clone') {
          git branch: 'main', credentialsId: 'git-prabhut', url: 'https://github.com/git-prabhut/bazel-examples'
     }

     stage ('Build') {
           sh 'ls "${WORKSPACE}"'
           sh '''
               cd "${WORKSPACE}/cpp-tutorial/stage1/main/"
               bazel build //main:hello-world 
           '''
     }
}
